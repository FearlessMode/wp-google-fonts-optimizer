{
  "name": "zytzagoo/wp-google-fonts-optimizer",
  "description": "Automatically detect and combine multiple Google Web Font requests into a single one.",
  "type": "wordpress-plugin",
  "homepage": "https://github.com/zytzagoo/wp-google-fonts-optimizer",
  "keywords": [
    "combine",
    "google-fonts",
    "google-web-fonts",
    "optimizer",
    "requests",
    "webfont",
    "wordpress",
    "wp"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "zytzagoo",
      "email": "hello@zytzagoo.net",
      "homepage": "https://zytzagoo.net"
    }
  ],
  "support": {
    "issues": "https://github.com/zytzagoo/wp-google-fonts-optimizer/issues"
  },
  "require": {
    "composer/installers" : "~1.0",
    "php": ">=5.4"
  },
  "require-dev": {
    "php": ">=5.6",
    "brain/monkey": "^2.0.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.2",
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "~4.8|~5.1",
    "wimg/php-compatibility": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "ZWF\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZWF\\GoogleFontsOptimizer\\Tests\\": "tests"
    }
  }
}
